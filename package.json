{
  "name": "@adempiere/grpc-web-store-api",
  "version": "1.4.1",
  "description": "ADempiere Web Store write in Javascript for a node service",
  "main": "index.js",
  "scripts": {
    "stub": "npm run stub:access && npm run stub:client && npm run stub:web_store",
    "stub:access": "grpc_tools_node_protoc proto/access.proto --js_out=import_style=commonjs,binary:src/grpc/ --grpc_out=grpc_js:src/grpc/",
    "stub:client": "grpc_tools_node_protoc proto/client.proto --js_out=import_style=commonjs,binary:src/grpc/ --grpc_out=grpc_js:src/grpc/",
    "stub:web_store": "grpc_tools_node_protoc proto/web_store.proto --js_out=import_style=commonjs,binary:src/grpc/ --grpc_out=grpc_js:src/grpc/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adempiere/gRPC-Web-Store-API.git"
  },
  "author": "Yamel Senih",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/adempiere/gRPC-Web-Store-API/issues"
  },
  "homepage": "https://github.com/adempiere/gRPC-Web-Store-API#readme",
  "dependencies": {
    "@grpc/grpc-js": "^1.2.12",
    "google-protobuf": "^3.15.6"
  },
  "devDependencies": {
    "grpc-tools": "^1.11.1"
  },
  "engines": {
    "node": ">= 10.0",
    "npm": ">= 6.0.0"
  },
  "keywords": [
    "compiere",
    "adempiere",
    "erp",
    "grpc",
    "webstore",
    "java",
    "web"
  ]
}
